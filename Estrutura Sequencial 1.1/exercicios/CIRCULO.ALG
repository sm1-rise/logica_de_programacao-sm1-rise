Algoritmo "circulo"

Var
   ValorRaio,Area: real
   


Inicio
      escreval ("***********")
      escreval ("APP CIRCULO")
      escreval ("***********")
      escreval ("")
      
      escreval ("Digite o valor do Raio do circulo: ")
      leia (ValorRaio)
      
      Area <- Pi * Exp(ValorRaio,2)
      
      Escreval ("AREA: ",Area:4:3)
      
      
      
      

Fimalgoritmo