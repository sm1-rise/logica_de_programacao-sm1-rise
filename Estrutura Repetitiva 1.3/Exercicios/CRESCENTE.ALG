Algoritmo "crescente"

Var
   x, y: inteiro


Inicio
escreval ("Digite dois números: ")
   leia (x)
   leia (y)
   
   enquanto x <> y faca
   se x > y entao
      escreval ("DECRESCENTE")
   senao
      escreval ("CRESCENTE")
   fimse

   escreval ("Digite dois números: ")
   leia (x)
   leia (y)
   fimenquanto
   

      


Fimalgoritmo